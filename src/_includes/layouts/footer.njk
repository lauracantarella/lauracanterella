<footer>
    <div class="foot-left">
        {%- for item in menuitems.copyright -%}
               <span id="{{item.name}}" class="f-menu">{{item.name}}</span>
        {%- endfor -%}
    </div>
    <div class="foot-right">
        {%- for item in menuitems.footer -%}  
            <a href="{{ item.target  | url }}"  class="f-menu foot-link">{{item.name}}</a>
        {%- endfor -%}
    </div>

</footer>